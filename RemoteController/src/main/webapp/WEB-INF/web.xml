<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RemoteControllerServlet</servlet-name>
  	<display-name>RemoteControllerServlet</display-name>
  	<description></description>
  	<servlet-class>org.mohsoft.servlet.RemoteControllerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addDevicesServlet</servlet-name>
  	<display-name>addDevicesServlet</display-name>
  	<description></description>
  	<servlet-class>org.mohsoft.servlet.addDevicesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addMacroServlet</servlet-name>
  	<display-name>addMacroServlet</display-name>
  	<description></description>
  	<servlet-class>org.mohsoft.servlet.addMacroServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RemoteControllerServlet</servlet-name>
  	<url-pattern>/start</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addDevicesServlet</servlet-name>
  	<url-pattern>/addDevicesServlet</url-pattern>
  </servlet-mapping>

</web-app>
